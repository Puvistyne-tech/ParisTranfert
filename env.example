# Vehicle Data Configuration
# Set to true to use Appwrite for vehicle data, false to use local data
NEXT_PUBLIC_USE_APPWRITE_VEHICLES=false

# Fallback to local data if Appwrite fails
NEXT_PUBLIC_FALLBACK_TO_LOCAL=true

# Cache timeout in milliseconds (5 minutes default)
NEXT_PUBLIC_VEHICLE_CACHE_TIMEOUT=300000

# Appwrite Configuration (only needed if using Appwrite)
NEXT_PUBLIC_APPWRITE_DATABASE_ID=your_database_id
NEXT_PUBLIC_APPWRITE_VEHICLES_COLLECTION_ID=vehicles
NEXT_PUBLIC_APPWRITE_VEHICLE_CATEGORIES_COLLECTION_ID=vehicle_categories

# Debug mode (optional)
NEXT_PUBLIC_DEBUG_VEHICLES=false

# OpenStreetMap/Nominatim Configuration
# No API key required - Nominatim is free and open source
# Rate limit: 1 request per second (please respect this limit)
